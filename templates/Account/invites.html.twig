<div>
    <button type="button" title="Not implemented" disabled>Request more</button>
</div>

<table>
    <thead>
    <tr>
        <th>#</th>
        <th><i class="fas fa-external-link-alt"></i></th>
    </tr>
    </thead>
    <tbody>
    {# @var invite \App\Entity\Invite #}
    {% for invite in invites %}
    <tr>
        <th scope="row">{{ loop.index }}</th>
        <td>
            {% if invite.usedBy %}
                Used by <strong>{{ invite.usedBy.username }}</strong>.
            {% else %}
                {% set invite_url = url('user_register', { code: invite.code }) %}
                <input type="url" value="{{ invite_url }}" readonly="readonly" onclick="this.select()">
            {% endif %}
        </td>
    </tr>
    {% endfor %}
    </tbody>
</table>
